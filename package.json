{
  "name": "customerbase",
  "version": "1.0.0",
  "description": "Simple GraphQL server",
  "main": "server.js",
  "author": "Simon Cateau",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "serve:dev": "nodemon server.js",
    "json:dev": "json-server --watch data.json",
    "serve": "concurrently \"yarn run serve:dev\" \"yarn run json:dev\""
  },
  "dependencies": {
    "axios": "^0.24.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^16.0.1",
    "json-server": "^0.17.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
